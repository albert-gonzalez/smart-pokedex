{
  "expo": {
    "name": "Smart Pokédex",
    "description": "Find you favorite Pokémon with this Pokédex",
    "slug": "smart-pokedex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#cc0d2d",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#cc0d2d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#cc0d2d"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "themecolor": "#dc0a2d"
    }
  }
}
